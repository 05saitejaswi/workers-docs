{
  "name": "cloudflare-workers-docs",
  "version": "1.0.0",
  "description": "Cloudflare Workers developer docs",
  "license": "Apache-2.0 OR MIT",
  "main": "workers-site/index.js",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "worker-build": "npm run build && wrangler build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "test": "ava workers-site/test/index.js --verbose",
    "start": "npm run develop",
    "worker-start": "npm run develop && wrangler preview --watch -e staging",
    "publish": "npm run build && wrangler publish -e"
  },
  "dependencies": {
    "gatsby": "^2.15.29",
    "gatsby-image": "^2.2.24",
    "gatsby-plugin-copy-files": "^1.0.3",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-offline": "^3.0.11",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-sharp": "^2.2.28",
    "gatsby-plugin-typescript": "^2.1.11",
    "gatsby-plugin-typography": "^2.3.11",
    "gatsby-source-filesystem": "^2.1.29",
    "gatsby-source-rest-api": "^0.2.2",
    "gatsby-transformer-remark": "^2.6.27",
    "gatsby-transformer-sharp": "^2.2.20",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "devDependencies": {
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-helmet": "^5.0.14",
    "glob": "^7.1.6",
    "prettier": "^1.18.2",
    "ava": "^2.4.0"
  },
  "keywords": [
    "gatsby",
    "workers",
    "cloudflare"
  ]
}